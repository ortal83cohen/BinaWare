<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:showIn="@layout/activity_main">
    <Transition
        app:constraintSetEnd="@id/end"
        app:constraintSetStart="@id/start"
        app:duration="300">

    </Transition>
    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/toolbar_layout"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:background="@color/white"
            android:elevation="3dp"
            android:transitionName="toolbar_layout"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Constraint
            android:id="@+id/fab1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/fab_margin"
            android:visibility="gone"
            app:elevation="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/fab"
            app:layout_constraintLeft_toLeftOf="@+id/fab"
            app:layout_constraintRight_toRightOf="@+id/fab"
            app:layout_constraintTop_toTopOf="@+id/fab" />
        <Constraint
            android:id="@+id/fab2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/fab_margin"
            android:visibility="gone"
            app:elevation="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/fab"
            app:layout_constraintLeft_toLeftOf="@+id/fab"
            app:layout_constraintRight_toRightOf="@+id/fab"
            app:layout_constraintTop_toTopOf="@+id/fab" />
        <Constraint
            android:id="@+id/fab3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/fab_margin"
            android:visibility="gone"
            app:elevation="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/fab"
            app:layout_constraintLeft_toLeftOf="@+id/fab"
            app:layout_constraintRight_toRightOf="@+id/fab"
            app:layout_constraintTop_toTopOf="@+id/fab" />

        <Constraint
            android:id="@+id/text1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:elevation="3dp"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="@+id/fab1"
            app:layout_constraintLeft_toLeftOf="@+id/fab1"
            app:layout_constraintTop_toTopOf="@+id/fab1" />
        <Constraint
            android:id="@+id/text2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:elevation="3dp"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="@+id/fab2"
            app:layout_constraintLeft_toLeftOf="@+id/fab2"
            app:layout_constraintTop_toTopOf="@+id/fab2" />

        <Constraint
            android:id="@+id/text3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:elevation="3dp"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="@+id/fab3"
            app:layout_constraintLeft_toLeftOf="@+id/fab3"
            app:layout_constraintTop_toTopOf="@+id/fab3" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/toolbar_layout"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@color/white"
            android:elevation="3dp"
            android:transitionName="toolbar_layout"
            app:layout_constraintBottom_toBottomOf="@id/fab3"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <Constraint
            android:id="@+id/fab1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:visibility="visible"
            app:elevation="10dp"
            app:layout_constraintLeft_toLeftOf="@+id/fab"
            app:layout_constraintRight_toRightOf="@+id/fab"
            app:layout_constraintTop_toBottomOf="@+id/fab" />
        <Constraint
            android:id="@+id/fab2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/fab_margin"
            android:visibility="visible"
            app:elevation="10dp"
            app:layout_constraintLeft_toLeftOf="@+id/fab"
            app:layout_constraintRight_toRightOf="@+id/fab"
            app:layout_constraintTop_toBottomOf="@+id/fab1" />
        <Constraint
            android:id="@+id/fab3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/fab_margin"
            android:visibility="visible"
            app:elevation="10dp"
            app:layout_constraintLeft_toLeftOf="@+id/fab"
            app:layout_constraintRight_toRightOf="@+id/fab"
            app:layout_constraintTop_toBottomOf="@+id/fab2" />
        <Constraint
            android:id="@+id/text1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="8dp"
            android:elevation="3dp"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@+id/fab1"
            app:layout_constraintLeft_toRightOf="@+id/fab1"
            app:layout_constraintTop_toTopOf="@+id/fab1" />
        <Constraint
            android:id="@+id/text2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="8dp"
            android:elevation="3dp"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@+id/fab2"
            app:layout_constraintLeft_toRightOf="@+id/fab2"
            app:layout_constraintTop_toTopOf="@+id/fab2" />

        <Constraint
            android:id="@+id/text3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="8dp"
            android:elevation="3dp"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@+id/fab3"
            app:layout_constraintLeft_toRightOf="@+id/fab3"
            app:layout_constraintTop_toTopOf="@+id/fab3" />
    </ConstraintSet>
</MotionScene>